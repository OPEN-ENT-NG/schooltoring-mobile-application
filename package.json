    "react-dom": "^16.4.1",
    "react-test-renderer": "16.3.1",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "start": "react-native start --port=8088",
    "android": "react-native run-android && adb reverse tcp:8081 tcp:8088",
