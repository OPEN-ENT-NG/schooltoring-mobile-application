{
  "name": "Schooltoring",
  "version": "1.0.1",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "jest": "^23.1.0",
    "jest-react-native": "^18.0.0",
    "prettier": "^1.13.5",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.1",
    "react-test-renderer": "16.3.1",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "start": "react-native start --port=8088 --reset-cache",
    "start:default": "react-native start --reset-cache",
    "android": "react-native run-android && adb reverse tcp:8081 tcp:8088",
    "android:default": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest --verbose --silent",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch --verbose --silent",
    "android:log": "react-native log-android",
    "android:dev": "npm run android && npm run android:log"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js",
      "<rootDir>/node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "<rootDir>/node_modules/appcenter-crashes/test/AppCenterCrashesMock.js",
      "<rootDir>/src/jest/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/jest/assetsTransformer.js"
    }
  },
  "dependencies": {
    "appcenter": "^1.8.0",
    "appcenter-analytics": "^1.8.0",
    "appcenter-crashes": "^1.8.0",
    "axios": "^0.18.0",
    "base-64": "^0.1.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-autocomplete-input": "^3.5.0",
    "react-native-firebase": "^4.3.8",
    "react-native-i18n": "^2.0.12",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^2.3.1",
    "react-navigation-redux-helpers": "^2.0.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  }
}
